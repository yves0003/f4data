{
  "scopeName": "source.rd",
  "patterns": [
    {
      "match": "#.*$",
      "name": "comment.definition.rd"
    },
    {
      "match": "(?i)\\b(table|enum|ref|as)\\b",
      "name": "keyword.control.rd"
    },
    {
      "match": "<|>",
      "name": "string.unquoted.rd"
    },
    {
      "match": "^\\s*(?:'([^']+)'|\"([^\"]+)\"|([-áéíóúÁÉÍÓÚàèìòùÀÈÌÒÙâêîôûÂÊÎÔÛäëïöüÄËÏÖÜA-Za-z0-9_]+))\\s*:\\s*([^\\[]*?)\\s*(?:\\[(.*?)\\])?\\s*$",
      "captures": {
        "1": { "name": "constant.enum.key.single-quoted.rd" },
        "2": { "name": "constant.enum.key.double-quoted.rd" },
        "3": { "name": "constant.enum.key.rd" },
        "4": { "name": "string.description.rd" },
        "5": { "name": "meta.annotation.brackets.rd" }
      }
    },
    {
      "match": "^\\s*([A-Za-z_][A-Za-z0-9_]*)\\s+([A-Za-z_][A-Za-z0-9_]*)(?:\\s+.*)?$",
      "captures": {
        "1": { "name": "variable.other.rd" },
        "2": { "name": "storage.type.rd" }
      }
    }
  ]
}
